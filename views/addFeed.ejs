<%- include('./components/header') %>
<div class="container">
  <h1 class="mb-4">Welcome to FaceBook</h1>
  <% if (result) { %>
      <div class="card">
          <div class="card-body">
            <h5 class="card-title"><%= result.name %></h5>
            <p class="card-text"><%= result.message %></p>
            
            <a href=/delete-feed/<%= result._id%> class="btn btn-danger">Delete</a>
            <a href=/edit-feed/<%= result._id%> class="btn btn-primary">Edit Post</a>
            
           
          </div>
        </div>
      <% } else { %>
              there is no record
  <% } %>
</div>
  <%- include('./components/footer') %>